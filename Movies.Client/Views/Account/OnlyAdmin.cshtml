@model UserInfoViewModel
@{
    ViewData["Title"] = "Only Admin";
}

<h1>Only Admin</h1>

<table class="table">
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.UserInfoDictionary)
        {
            <tr>
                <td>@item.Key</td>
                @* @Html.DisplayFor(modelItem => item.Key); *@
                <td>@item.Value</td>
                @* @Html.DisplayFor(modelItem => item.Value); *@
            </tr>
        }
    </tbody>
</table>